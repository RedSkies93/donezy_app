root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{dart,yaml,yml,md,txt}]
indent_style = space
indent_size = 2

[*.gradle]
indent_style = space
indent_size = 2

[*.{png,jpg,jpeg,gif,webp,ico}]
trim_trailing_whitespace = false
insert_final_newline = false
